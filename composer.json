{
    "name": "macramejs/macrame-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Carstens-Behrens",
            "email": "lennart.carbe@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Macrame\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php": "^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "illuminate/support": "^8.46|^9.0",
        "illuminate/contracts": "^8.46|^9.0",
        "inertiajs/inertia-laravel": "^0.5|^0.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macrame\\MacrameServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
